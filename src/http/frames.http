Content-Type: application/json

{
  "brand": "RayBan",
  "model": "RB123",
  "material": "metal",
  "color": "black",
  "price": 200,
  "stock": 5,
  "supplier_id": 1,
  "image": "frame.jpg",
  "status": "ACTIVE"
}

